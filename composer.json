{
    "name": "gr4vy/gr4vy-php",
    "description": "Welcome to the Gr4vy PHP SDK",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://openapi-generator.tech",
    "license": "MIT",
    "type":"library",
    "authors": [
        {
            "name": "Gr4vy Inc",
            "homepage": "https://gr4vy.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "lcobucci/jwt": "^3.4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Gr4vy\\": "lib/",
            "Gr4vy\\Api\\": "lib/api/",
            "Gr4vy\\Model\\": "lib/model/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Gr4vy\\Test\\" : "test/" }
    }
}
